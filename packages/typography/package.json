{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "@grapos/typography",
  "version": "0.0.0-1",
  "type": "module",
  "main": "index.js",
  "description": "A flexible and fun to use Material Design-based SolidJS component library",
  "license": "MIT",
  "keywords": [
    "ui",
    "components",
    "solid",
    "solidjs",
    "solid-js",
    "typesafe",
    "material",
    "material design",
    "flexible components"
  ],
  "jest": {
    "preset": "solid-jest/preset/browser",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json && node-sass ./src -o ./lib  && node ../convert-scss-to-css-in-build.js && cp package.json lib/package.json ",
    "prepublish": "npm run build && cp README.md lib/README.md",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielmfern/grapes.git"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "bugs": {
    "url": "https://github.com/gabrielmfern/grapes/issues"
  },
  "readme": "https://raw.githubusercontent.com/gabrielmfern/GrapeS/master/README.md",
  "homepage": "https://github.com/gabrielmfern/grapes#readme",
  "author": "Gabriel Miranda",
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@types/jest": "^29.5.3",
    "@types/tinycolor2": "^1.4.3",
    "jest": "^29.6.2",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "jest-environment-jsdom": "^29.6.2",
    "node-sass": "^9.0.0",
    "sass": "^1.66.1",
    "solid-jest": "^0.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@grapos/utils": "workspace:latest",
    "@grapos/core": "workspace:latest",
    "@grapos/layout": "workspace:latest",
    "solid-js": "^1.7.11"
  }
}
