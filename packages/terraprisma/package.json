{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "terraprisma",
  "version": "0.0.0-testing-20231229055905",
  "description": "A flexible and fun to use batteries included SolidJS component library",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "private": false,
  "keywords": [
    "ui",
    "components",
    "solid",
    "solidjs",
    "solid-js",
    "typesafe",
    "material",
    "material design",
    "flexible components"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint:types": "tsc",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier . --write",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielmfern/terraprisma.git"
  },
  "bugs": {
    "url": "https://github.com/gabrielmfern/terraprisma/issues"
  },
  "homepage": "https://github.com/gabrielmfern/terraprisma#readme",
  "author": "Gabriel Miranda",
  "devDependencies": {
    "@solidjs/testing-library": "^0.7.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/tinycolor2": "^1.4.3",
    "autoprefixer": "^10.4.16",
    "esbuild-postcss": "0.0.4",
    "esbuild-style-plugin": "1.6.3",
    "jsdom": "^22.1.0",
    "postcss": "8.4.33",
    "postcss-scss": "4.0.9",
    "prettier": "^3.0.3",
    "regenerator-runtime": "0.13.11",
    "sass": "^1.66.1",
    "solid-js": "^1.8.7",
    "tailwindcss": "^3.3.5",
    "tsup": "^8.0.1",
    "tsup-preset-solid": "2.2.0",
    "typescript": "^5.3.3",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@csstools/postcss-sass": "5.1.1",
    "@solid-primitives/input-mask": "^0.2.0",
    "@solid-primitives/props": "^3.1.8",
    "@solid-primitives/refs": "^1.0.5",
    "solid-transition-group": "^0.2.3",
    "tailwind-merge": "^2.0.0",
    "tinycolor2": "^1.6.0"
  },
  "overrides": {
    "@rollup/plugin-node-resolve": "13.3.0"
  },
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "types": "./dist/index.d.ts",
  "browser": {
    "./dist/server.js": "./dist/index.js",
    "./dist/server.cjs": "./dist/index.cjs"
  },
  "exports": {
    ".": {
      "worker": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/server.cjs"
        }
      },
      "browser": {
        "solid": {
          "development": "./dist/dev.jsx",
          "import": "./dist/index.jsx"
        },
        "development": {
          "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/dev.js"
          },
          "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/dev.cjs"
          }
        },
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      },
      "deno": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/server.cjs"
        }
      },
      "node": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/server.cjs"
        }
      },
      "solid": {
        "development": "./dist/dev.jsx",
        "import": "./dist/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/dev.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/dev.cjs"
        }
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css",
      "default": "./dist/index.css"
    }
  },
  "typesVersions": {},
  "type": "module"
}
