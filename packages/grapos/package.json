{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "grapos",
  "version": "0.0.7",
  "type": "module",
  "main": "index.jsx",
  "description": "A flexible and fun to use Material Design-basd SolidJS component library",
  "license": "MIT",
  "keywords": [
    "ui",
    "components",
    "solid",
    "solidjs",
    "solid-js",
    "typesafe",
    "material",
    "material design",
    "flexible components"
  ],
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json && node-sass ./src -o ./lib && node ./convert-scss-to-css-in-build.js",
    "prepublish": "npm run build && cp package.json lib/package.json && cp ../../README.md README.md",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielmfern/grapes.git"
  },
  "devDependencies": {
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "^0.12.1",
    "node-sass": "^9.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "typescript": "^5.2.0-dev.20230805"
  },
  "dependencies": {
    "@solid-primitives/input-mask": "^0.2.0",
    "@solid-primitives/mutation-observer": "^1.1.13",
    "material-symbols": "^0.5.5",
    "solid-js": "^1.7.8",
    "solid-transition-group": "^0.2.2",
    "tinycolor2": "^1.6.0"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "bugs": {
