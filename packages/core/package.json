{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "@grapos/core",
  "version": "0.0.0-1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "main": "index.cjs",
  "description": "A flexible and fun to use Material Design-based SolidJS component library",
  "license": "MIT",
  "keywords": [
    "ui",
    "components",
    "solid",
    "solidjs",
    "solid-js",
    "typesafe",
    "material",
    "material design",
    "flexible components"
  ],
  "jest": {
    "preset": "solid-jest/preset/browser",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "scripts": {
    "build": "vite build && cp ./package.json lib/package.json",
    "prepublish": "npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielmfern/grapes.git"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "bugs": {
    "url": "https://github.com/gabrielmfern/grapes/issues"
  },
  "readme": "https://raw.githubusercontent.com/gabrielmfern/GrapeS/master/README.md",
  "homepage": "https://github.com/gabrielmfern/grapes#readme",
  "author": "Gabriel Miranda",
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@types/jest": "^29.5.3",
    "@types/tinycolor2": "^1.4.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "sass": "^1.66.1",
    "solid-jest": "^0.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^3.5.2",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-solid": "^2.7.0"
  },
  "dependencies": {
    "@grapos/layout": "workspace:latest",
    "@grapos/transitions": "workspace:latest",
    "@grapos/utils": "workspace:latest",
    "solid-js": "^1.7.11",
    "tinycolor2": "^1.6.0"
  }
}
